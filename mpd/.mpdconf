music_directory         "~/Music/mpd"
playlist_directory      "~/.mpd/playlists"
db_file                 "~/.mpd/database"
pid_file                "~/.mpd/pid"
log_file                "~/.mpd/log"
state_file              "~/.mpd/state"
sticker_file            "~/.mpd/sticker.sql"
port                    "6600"
auto_update             "yes"
audio_output {
    type            "osx"
    name            "Sven IV"
    mixer_type      "software"
}

decoder {
    plugin "ffmpeg"
    enabled "yes"
}
